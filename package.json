{
	"name": "nextstage-technical",
	"private": true,
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"scripts": {
		"dev": "npx prisma generate && dotenv -e .env.local -- prisma migrate reset --force && next dev",
		"prisma:reset": "npx prisma generate && dotenv -e .env.local -- prisma migrate reset",
		"prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
		"test:watch": "jest --watch",
		"test": "dotenv -e .env.local -- jest",
		"build": "next build",
		"start": "next start",
		"lint": "next lint"
	},
	"dependencies": {
		"@prisma/client": "3.9.0",
		"dotenv-cli": "^4.1.1",
		"fetch": "^1.1.0",
		"jest-mock-extended": "^2.0.4",
		"lodash": "4.17.21",
		"next": "12.0.10",
		"react": "17.0.2",
		"react-beautiful-dnd": "13.1.0",
		"react-dom": "17.0.2",
		"scss": "0.2.4"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@types/lodash": "4.14.178",
		"@types/node": "17.0.14",
		"@types/react": "17.0.38",
		"@types/react-beautiful-dnd": "13.1.2",
		"@types/react-dom": "^17.0.11",
		"eslint": "8.8.0",
		"eslint-config-next": "12.0.10",
		"jest": "^27.4.7",
		"node-mocks-http": "^1.11.0",
		"prisma": "3.9.0",
		"sass": "^1.49.7",
		"ts-jest": "^27.1.3",
		"ts-node": "10.4.0",
		"typescript": "4.5.5"
	}
}
